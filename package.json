{
  "name": "footballbin-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for FootballBin football match predictions",
  "main": "dist/handler.js",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "deploy:dev": "sam deploy --template-file template.yaml --stack-name footballbin-mcp-dev --parameter-overrides Stage=dev",
    "deploy:prod": "sam deploy --template-file template.yaml --stack-name footballbin-mcp-prod --parameter-overrides Stage=prod"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "football",
    "soccer",
    "premier-league",
    "champions-league",
    "predictions",
    "ai",
    "claude"
  ],
  "author": "FootballBin",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.700.0",
    "@aws-sdk/lib-dynamodb": "^3.700.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.145",
    "@types/node": "^22.10.0",
    "typescript": "^5.7.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
